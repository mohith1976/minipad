
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
   id        String   @id @default(uuid())
  code      String   @unique
  content   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  
}